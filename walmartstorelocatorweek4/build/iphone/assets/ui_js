var storeUI=function(e){console.log(e);var o=Ti.UI.createWindow({backgroundColor:"#fff",layout:"horizontal"});for(i=0,j=e.length=4;i<j;i++){var a=Ti.UI.createView({height:350,width:350,top:40,left:20,backgroundColor:"#2277cc",layout:"vertical"}),r=Ti.UI.createLabel({font:{fontSize:30,fontWeight:"bold"},color:"#fff",text:e[i].name,left:20,top:20}),t=Ti.UI.createLabel({font:{fontSize:24},color:"#fff",text:e[i].address,left:20,right:20,top:20}),n=Ti.UI.createLabel({font:{fontSize:24},color:"#fff",text:e[i].city,left:20,top:20}),l=Ti.UI.createLabel({font:{fontSize:24},color:"#fff",text:e[i].zip,top:0,left:200}),s=Ti.UI.createLabel({font:{fontSize:18,fontWeight:"bold"},color:"#fff",text:e[i].phone,left:20,top:20}),d=Ti.UI.createView({height:50,width:200,backgroundColor:"#eee",top:20}),c=Ti.UI.createLabel({text:"Take Receipt Photo",color:"#000"});d.addEventListener("click",function(e){Ti.Media.showCamera({success:function(e){alert("Your photo was saved to the Photo Gallery")},cancel:function(){},error:function(e){var o;o=e.code==Ti.Media.NO_CAMERA?"This device does not have a camera or recording capabilities":"Unexpected error: "+e.code,Ti.UI.createAlertDialog({title:"Camera",message:o}).show()},saveToPhotoGallery:!0,allowEditing:!0,mediaTypes:[Ti.Media.MEDIA_TYPE_VIDEO,Ti.Media.MEDIA_TYPE_PHOTO]})}),a.add(r),a.add(t),a.add(n),n.add(l),a.add(s),a.add(d),d.add(c),o.add(a)}o.open()};exports.storeUI=storeUI;