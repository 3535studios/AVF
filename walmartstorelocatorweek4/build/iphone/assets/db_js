var save=function(e){var a=Ti.Database.open("walmartdb");for(i=0,j=e.length;i<j;i++)a.execute("CREATE TABLE IF NOT EXISTS storeTbl (id INTEGER PRIMARY KEY, name TEXT, address TEXT, city TEXT, zip INTEGER, phone INTEGER)"),a.execute("INSERT INTO storeTbl (name, address, city, zip, phone) VALUES(?,?,?,?,?)",e[i].name,e[i].address,e[i].city,e[i].zip,e[i].phone);a.close(),console.log(save),read()};exports.save=save;var read=function(){var e=Ti.Database.open("walmartdb");e.execute("CREATE TABLE IF NOT EXISTS storeTbl (id INTEGER PRIMARY KEY, name TEXT, address TEXT, city TEXT, zip INTEGER, phone INTEGER)");var a=e.execute("SELECT name, address, city, zip, phone FROM storeTbl");if(a.rowCount>0){for(var o=[];a.isValidRow();){var r={name:a.fieldByName("name"),address:a.fieldByName("address"),city:a.fieldByName("city"),zip:a.fieldByName("zip"),phone:a.fieldByName("phone")};o.push(r),a.next()}var i=require("ui");i.storeUI(o)}a.close(),e.close()};exports.read=read;var cleandb=function(){var e=Ti.Database.open("walmartdb");e.execute("DROP TABLE IF EXISTS storeTbl"),e.close()};exports.cleandb=cleandb;