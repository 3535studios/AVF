var apiData=function(e,o){console.log(apiData);var t="http://api.wunderground.com/api/2ea7d8e37d25e57a/conditions/q/"+e+","+o+".json";console.log(t);var r=Ti.Network.createHTTPClient({onload:function(e){var o=JSON.parse(this.responseText);console.log(o);var t={city:o.current_observation.display_location.city,temp:o.current_observation.temp_f,weather:o.current_observation.weather,weather_icon:o.current_observation.icon_url,humidity:o.current_observation.relative_humidity,wind:o.current_observation.wind_mph,feelslike:o.current_observation.feelslike_f};console.log(t);var r=require("ui");r.buildUI(t)},onerror:function(e){alert("The api is not connected.")},timeout:5e3});r.open("GET",t),r.send()};exports.apiData=apiData;