var buildUI=function(e){console.log(buildUI);var t=Ti.UI.createWindow({backgroundColor:"#f2f2f0",layout:"vertical"});if("android"===Ti.Platform.osname)var o=Ti.UI.createLabel({text:e.city,backgroundColor:"#f2c641",color:"#fff",height:100,top:0,width:"100%",textAlign:"center",font:{fontSize:64}});else var o=Ti.UI.createLabel({text:e.city,backgroundColor:"#f2c641",color:"#fff",height:100,top:20,width:"100%",textAlign:"center",font:{fontSize:64}});var i=Ti.UI.createLabel({text:e.temp+"°",backgroundColor:"#96d9ad",color:"#fff",height:200,width:"100%",textAlign:"center",font:{fontSize:164}}),r=Ti.UI.createLabel({text:e.weather,backgroundColor:"#f26b5e",color:"#fff",height:100,width:"100%",textAlign:"center",font:{fontSize:64}}),a=Ti.UI.createImageView({image:e.weather_icon,right:"30%"}),n=Ti.UI.createLabel({text:"Humidity: "+e.humidity,backgroundColor:"#6a777a",color:"#fff",height:100,width:"100%",textAlign:"center",font:{fontSize:44}}),l=Ti.UI.createLabel({text:"Wind MPH: "+e.wind,backgroundColor:"#64350d",color:"#fff",height:100,width:"100%",textAlign:"center",font:{fontSize:44}}),c=Ti.UI.createLabel({text:"Feels Like: "+e.feelslike+"°",backgroundColor:"#74bfd4",color:"#fff",height:100,width:"100%",textAlign:"center",font:{fontSize:44}});t.add(o),t.add(i),t.add(r),r.add(a),t.add(n),t.add(l),t.add(c),t.open()};exports.buildUI=buildUI;