var buildUI=function(e){console.log(buildUI);var i=Ti.UI.createWindow({backgroundColor:"#f2f2f0",layout:"vertical"});if("android"===Ti.Platform.osname)var t=Ti.UI.createLabel({text:e[0].city,backgroundColor:"#f2c641",color:"#fff",height:100,top:0,width:"100%",textAlign:"center",font:{fontSize:64}});else var t=Ti.UI.createLabel({text:e[0].city,backgroundColor:"#f2c641",color:"#fff",height:100,top:20,width:"100%",textAlign:"center",font:{fontSize:64}});var r=Ti.UI.createLabel({text:e[0].temp+"°",backgroundColor:"#96d9ad",color:"#fff",height:200,width:"100%",textAlign:"center",font:{fontSize:164}}),n=Ti.UI.createLabel({text:e[0].weather,backgroundColor:"#f26b5e",color:"#fff",height:100,width:"100%",textAlign:"center",font:{fontSize:64}}),a=Ti.UI.createLabel({text:"Humidity: "+e[0].humidity+"  Wind MPH: "+e[0].wind+"  Feels like: "+e[0].feelslike+"°",backgroundColor:"#6a777a",color:"#fff",height:100,width:"100%",textAlign:"center",font:{fontSize:20}}),o=Ti.UI.createLabel({text:"Wind Direction: "+e[0].wind_dir+"  Wind Gust: "+e[0].wind_gust+"  Wind Degrees: "+e[0].wind_degrees,backgroundColor:"#64350d",color:"#fff",height:100,width:"100%",textAlign:"center",font:{fontSize:20}}),d=Ti.UI.createLabel({text:"Visibility: "+e[0].visibility+"  Pressure: "+e[0].pressure+"  Dewpoint: "+e[0].dewpoint,backgroundColor:"#74bfd4",color:"#fff",height:100,width:"100%",textAlign:"center",font:{fontSize:20}}),l=Ti.UI.createLabel({text:"Wind Chill: "+e[0].windchill+"  Precipitation: "+e[0].rain+"  Heat Index: "+e[0].heat_index,backgroundColor:"#f2c641",color:"#fff",height:100,width:"100%",textAlign:"center",font:{fontSize:20}});i.add(t),i.add(r),i.add(n),i.add(a),i.add(o),i.add(d),i.add(l),i.open()};exports.buildUI=buildUI;