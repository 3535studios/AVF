var apiData=function(e,r){console.log(apiData);var i="http://api.wunderground.com/api/2ea7d8e37d25e57a/conditions/q/"+e+","+r+".json";console.log(i);var n=Ti.Network.createHTTPClient({onload:function(e){var r=JSON.parse(this.responseText);console.log(r);var i={city:r.current_observation.display_location.city,temp:r.current_observation.temp_f,weather:r.current_observation.weather,humidity:r.current_observation.relative_humidity,wind:r.current_observation.wind_mph,feelslike:r.current_observation.feelslike_f,wind_dir:r.current_observation.wind_dir,wind_gust_mph:r.current_observation.wind_gust_mph,wind_degrees:r.current_observation.wind_degrees,visibility:r.current_observation.visibility_mi,pressure:r.current_observation.pressure_in,dewpoint:r.current_observation.dewpoint_f,windchill:r.current_observation.windchill_f,rain:r.current_observation.precip_today_in,heat_index:r.current_observation.heat_index_f},n=require("db");n.cleandb(),console.log(i);var t=require("db");t.save(i)},onerror:function(e){alert("The api is not connected.")},timeout:5e3});n.open("GET",i),n.send()};exports.apiData=apiData;