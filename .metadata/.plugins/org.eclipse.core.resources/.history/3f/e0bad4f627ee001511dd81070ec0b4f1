var storeUI = function(storeInfo) {
	
	var win = Ti.UI.createWindow({
		backgroundColor: "#fff",
		layout: "vertical"
	});
	
	var storeView = Ti.UI.createView({
		height: 350,
		width: 350,
		top: 20,
		left: 20,
		backgroundColor: "#2277cc"
	});
	
	win.add(storeView);
	
	win.open();
};

exports.storeUI = storeUI;
