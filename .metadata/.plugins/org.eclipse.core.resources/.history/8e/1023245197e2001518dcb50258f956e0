//get geoData

var getGeo = function() {
	
	Ti.Geolocation.purpose = "Please turn on your location services."; 
	Ti.Geolocation.getCurrentPosition(function(pos){
                console.log(pos); 
		var lat = pos.coords.latitude;
		var lng = pos.coords.longitude;
        
        var apiModule = require("api");
			apiModule.apiData(lat, lng); 
	});
}; 

exports.getGeo = getGeo;
